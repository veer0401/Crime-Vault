<!-- Footer: Start -->
<footer class="landing-footer bg-body footer-text">
  <div class="footer-top position-relative overflow-hidden z-1">
    <img src="~/img/front-pages/backgrounds/footer-bg-light.png" alt="footer bg" class="footer-bg banner-bg-img z-n1" data-app-light-img="front-pages/backgrounds/footer-bg-light.png" data-app-dark-img="front-pages/backgrounds/footer-bg-dark.png" />
    <div class="container">
      <div class="row gx-0 gy-6 g-lg-10">
        <div class="col-lg-5">
          <a asp-controller="Dashboards" asp-action="Index" class="app-brand-link mb-6">
            <span class="app-brand-logo demo">@await Html.PartialAsync("../_Partials/_Macros")</span>
            <span class="app-brand-text demo footer-link fw-bold ms-2 ps-1">@TempData.Peek("appName")</span>
          </a>
          <p class="footer-text footer-logo-description mb-6">
            Most developer friendly & highly customisable Admin Dashboard Template.
          </p>
          <form class="footer-form">
            <label for="footer-email" class="small">Subscribe to newsletter</label>
            <div class="d-flex mt-1">
              <input type="email" class="form-control rounded-0 rounded-start-bottom rounded-start-top" id="footer-email" placeholder="Your email" />
              <button type="submit" class="btn btn-primary shadow-none rounded-0 rounded-end-bottom rounded-end-top">
                Subscribe
              </button>
            </div>
          </form>
        </div>
        <div class="col-lg-2 col-md-4 col-sm-6">
          <h6 class="footer-title mb-6">Demos</h6>
          <ul class="list-unstyled">
            <li class="mb-4">
              <a href="javascript:void(0);" class="footer-link">Vertical Layout</a>
            </li>
            <li class="mb-4">
              <a href="javascript:void(0);" class="footer-link">Horizontal Layout</a>
            </li>
            <li class="mb-4">
              <a href="javascript:void(0);" class="footer-link">Bordered Layout</a>
            </li>
            <li class="mb-4">
              <a href="javascript:void(0);" class="footer-link">Semi Dark Layout</a>
            </li>
            <li class="mb-4">
              <a href="javascript:void(0);" class="footer-link">Dark Layout</a>
            </li>
          </ul>
        </div>
        <div class="col-lg-2 col-md-4 col-sm-6">
          <h6 class="footer-title mb-6">Pages</h6>
          <ul class="list-unstyled">
            <li class="mb-4">
              <a asp-controller="FrontPages" asp-action="PricingPage" class="footer-link">Pricing</a>
            </li>
            <li class="mb-4">
              <a asp-controller="FrontPages" asp-action="PaymentPage" class="footer-link">Payment<span class="badge bg-primary ms-2">New</span></a>
            </li>
            <li class="mb-4">
              <a asp-controller="FrontPages" asp-action="CheckoutPage" class="footer-link">Checkout</a>
            </li>
            <li class="mb-4">
              <a asp-controller="FrontPages" asp-action="HelpCenterLanding" class="footer-link">Help Center</a>
            </li>
            <li class="mb-4">
              <a asp-controller="Auth" asp-action="LoginCover" target="_blank" class="footer-link">Login/Register</a>
            </li>
          </ul>
        </div>
        <div class="col-lg-3 col-md-4">
          <h6 class="footer-title mb-6">Download our app</h6>
          <a href="javascript:void(0);" class="d-block mb-4"><img src="~/img/front-pages/landing-page/apple-icon.png" alt="apple icon" /></a>
          <a href="javascript:void(0);" class="d-block"><img src="~/img/front-pages/landing-page/google-play-icon.png" alt="google play icon" /></a>
        </div>
      </div>
    </div>
  </div>
  <div class="footer-bottom py-3 py-md-5">
    <div class="container d-flex flex-wrap justify-content-between flex-md-row flex-column text-center text-md-start">
      <div class="mb-2 mb-md-0">
        <span class="footer-bottom-text">©
          <script>
          document.write(new Date().getFullYear());

          </script>
        </span>
        <a href='@TempData.Peek("authorUrl")' target="_blank" class="fw-medium text-white text-white">@TempData.Peek("authorName"),</a>
        <span class="footer-bottom-text"> Made with ❤️ for a better web.</span>
      </div>
      <div>
        <a href='@TempData.Peek("githubFreeUrl")' class="me-3" target="_blank"><img src="~/img/front-pages/icons/github.svg" alt="github icon" /></a>
        <a href='@TempData.Peek("facebookUrl")' class="me-3" target="_blank"><img src="~/img/front-pages/icons/facebook.svg" alt="facebook icon" /></a>
        <a href='@TempData.Peek("twitterUrl")' class="me-3" target="_blank"><img src="~/img/front-pages/icons/twitter.svg" alt="twitter icon" /></a>
        <a href='@TempData.Peek("instagramUrl")' target="_blank"><img src="~/img/front-pages/icons/instagram.svg" alt="google icon" /></a>
      </div>
    </div>
  </div>
</footer>
<!-- Footer: End -->
